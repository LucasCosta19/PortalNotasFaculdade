@{
    if (Session["UsuarioId"] == null)
    {       
        Response.Write("<script>location.href='http://localhost:51948/';</script>");
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Portal de Notas</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/style.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/styleResp.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css">    
    <script src="~/Scripts/NotasCalc.js"></script>
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.1/css/bootstrap.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.19/css/dataTables.bootstrap4.min.css" />
    <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Scripts/MyDataTables.js"></script>
    @RenderSection("styles", false)
    <style>
        .btn-primary {
            background: #093c25 !important;
            box-shadow: 5px 5px 15px #000 !important;
            border: none !important;
            padding-bottom: 10px;
            padding-top: 10px;
        }

        .btn-primary:hover {
            box-shadow: 5px 5px #ccc !important;
        }
    </style>
</head>
<body>
    @Html.Partial("_MenuRender")

    <div class="container body-content">
        @RenderBody()
    </div>

    @Html.Partial("_FooterRender")

    @RenderSection("ScriptPage", false)

    <style>
        .table-striped > tbody > tr:nth-child(2n+1) > td,
        .table-striped > tbody > tr:nth-child(2n+1) > th {
            background-color: #f4f4f4;
            color: #000;
        }
    </style>
</body>
</html>